{
  "name": "tested-storybook",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "5.5.0",
    "@testing-library/react": "10.0.4",
    "@testing-library/user-event": "10.1.0",
    "chai": "4.2.0",
    "hermione": "3.3.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "react-scripts": "3.4.1",
    "typescript": "3.8.3"
  },
  "scripts": {
    "start": "PORT=12345 react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 12345",
    "selenium": "selenium-standalone install && selenium-standalone start",
    "create-tests": "webpack --mode production",
    "lint:es": "eslint src/**/*.{ts,tsx}",
    "fix:es": "eslint src/**/*.{ts,tsx} --fix",
    "test:storybook": "npm run selenium & npm run storybook & npm run create-tests && node_modules/.bin/hermione --set storybook gui",
    "test:app": "npm run selenium & npm run start & node_modules/.bin/hermione --set app gui",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@storybook/react": "5.3.18",
    "@types/jest": "25.2.1",
    "@types/react-helmet": "6.0.0",
    "@types/react-test-renderer": "16.9.2",
    "@types/testing-library__jest-dom": "5.0.3",
    "@typescript-eslint/eslint-plugin": "3.0.0",
    "@typescript-eslint/parser": "3.0.0",
    "babel-jest": "24.9.0",
    "babel-loader": "8.1.0",
    "codecov.io": "0.1.6",
    "coveralls": "3.1.0",
    "css-loader": "3.5.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "2.5.1",
    "fork-ts-checker-webpack-plugin": "4.1.3",
    "html-reporter": "5.15.3",
    "husky": "4.2.5",
    "jest": "24.9.0",
    "jest-transform-css": "2.0.0",
    "lint-staged": "10.2.6",
    "react-test-renderer": "16.13.1",
    "storytests-webpack-plugin": "^1.0.6",
    "ts-jest": "25.4.0",
    "ts-loader": "7.0.2",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
